	pmid = {23818599},
	keywords = {cavity QED, critical behavior, Dicke model, driven-dissipative phase transitions, quantum gas},
	pages = {11763--11767},
	file = {Full Text PDF:/Users/tureci/Library/Application Support/Firefox/Profiles/k0m1f40n.default/zotero/storage/DS7ENIUH/Brennecke et al. - 2013 - Real-time observation of fluctuations at the drive.pdf:application/pdf;Snapshot:/Users/tureci/Library/Application Support/Firefox/Profiles/k0m1f40n.default/zotero/storage/2BTV9RPF/11763.html:text/html}
