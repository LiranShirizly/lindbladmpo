@article{MarinoDiehlPRL16,
	title = {Driven {Markovian} {Quantum} {Criticality}},
	volume = {116},
	url = {https://link.aps.org/doi/10.1103/PhysRevLett.116.070407},
	doi = {10.1103/PhysRevLett.116.070407},
