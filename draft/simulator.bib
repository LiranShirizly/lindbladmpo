@article{Yazdani_Science,
author = {Pasupathy, Abhay N. and Pushp, Aakash and Gomes, Kenjiro K. and Parker, Colin V. and Wen, Jinsheng and Xu, Zhijun and Gu, Genda and Ono, Shimpei and Ando, Yoichi and Yazdani, Ali},
title = {{Electronic Origin of the Inhomogeneous Pairing Interaction in the High-Tc Superconductor BiSCCO}},
journal = {Science},
volume = {320},
number = {5873},
pages = {196-201},
doi = {10.1126/science.1154700},
year = {2008},
abstract = {Identifying the mechanism of superconductivity in the high-temperature cuprate superconductors is one of the major outstanding problems in physics. We report local measurements of the onset of superconducting pairing in the high-transition temperature (Tc) superconductor BiSCCO using a lattice-tracking spectroscopy technique with a scanning tunneling microscope. We can determine the temperature dependence of the pairing energy gaps, the electronic excitations in the absence of pairing, and the effect of the local coupling of electrons to bosonic excitations. Our measurements reveal that the strength of pairing is determined by the unusual electronic excitations of the normal state, suggesting that strong electron-electron interactions rather than low-energy (<0.1 volts) electron-boson interactions are responsible for superconductivity in the cuprates.}
}


